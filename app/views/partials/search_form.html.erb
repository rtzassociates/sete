<%= form_tag users_path, :method => 'get', :class => "form-search", :autocomplete => "off" do %>
  <div class="input-append">
    <%= text_field_tag :search, params[:search], data: { :source => collection.order(:name).map(&:name) } %>
    <%= submit_tag "Search", :name => nil, :class => "btn btn-primary" %>
  </div>
<% end %>